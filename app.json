{
  "expo": {
    "name": "Raccoon",
    "slug": "raccoon",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "raccoon",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8749426160957410~3140736290"
      },
      "bundleIdentifier": "com.raccoon.plus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.raccoon.plus",
      "versionCode": 2,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_PHONE_STATE",
        "RECORD_AUDIO",
        "READ_CONTACTS",
        "BLUETOOTH",
        "BLUETOOTH_ADMIN"
      ],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8749426160957410~1214125116"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
